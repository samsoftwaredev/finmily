{
  "name": "finmily",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/app.js",
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-jwt": "^7.7.5",
    "pg": "^8.7.3",
    "typeorm": "^0.3.7",
    "typescript-json-schema": "^0.54.0",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.5",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.19.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.6",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "tsc": "./node_modules/typescript/bin/tsc",
    "compile": "tsc",
    "postinstall": "npm run compile",
    "compileDev": "tsc -w",
    "coverage": "DOTENV_CONFIG_PATH=.env.test.local jest --coverage --setupFiles=dotenv/config",
    "schema": "ts-node ./schemaGenerator.ts",
    "build": "npm run postinstall",
    "start": "npm run schema && node .",
    "dev": "npm run schema && nodemon --watch './**/*.ts' --exec 'ts-node' src/app.ts",
    "test": "DOTENV_CONFIG_PATH=.env.test.local jest --runInBand --detectOpenHandles --setupFiles=dotenv/config --verbose --watch"
  },
